{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "space-before-function-paren": "off",
        "eol-last": "off",
        "no-multiple-empty-lines": "off",
        "no-trailing-spaces": "off",
        "padded-blocks": "off",
        "no-unused-vars": "warn",
        "comma-spacing": "warn",
        "react/prop-types": "off",
        "comma-dangle": "warn",
        "spaced-comment": "warn",
        "no-empty": "warn"
    }
}